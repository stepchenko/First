@model QueueStepchenko.Models.User

@if (Model.RoleName == "client")
{
    @Html.ActionLink(Model.Name, "ProfileClient", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" })
}
else
{
    @Html.ActionLink(Model.Name, "ProfileEmployee", "Employee", routeValues: new { login = Model.Login }, htmlAttributes: new { id = "loginLink" })
}


@Html.ActionLink("Выйти", "LogOff", "Account", routeValues: null, htmlAttributes: new { id = "logoffLink" })


