@model QueueStepchenko.Models.User

@if (Model.RoleName == "client")
{
    @Html.ActionLink(Model.Name, "Profile", "Account", routeValues: null, htmlAttributes: new { id = "loginLink" })
}
else
{
    @Html.ActionLink(Model.Name, "Profile", "Employee", routeValues: new {login = Model.Login}, htmlAttributes: new { id = "loginLink" })
}


@Html.ActionLink("Выйти", "LogOff", "Account", routeValues: null, htmlAttributes: new { id = "logoffLink" })


